  "description": "Demo Wallet published as web component",
  "main": "./dist/demo-wallet.umd.min.js",
  "module": "./dist/demo-wallet.esm.min.js",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.5",
    "glob-parent": "^6.0.1",
    "live-server": "^1.2.1",
    "rollup": "^2.57.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.43.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "sh=./scripts/build.sh; chmod +x $sh && $sh ./src ./dist",
    "serve:local": "sh=./scripts/serve.sh; chmod +x $sh && $sh 1234 ./html/local.html ./dist",
    "serve:public": "sh=./scripts/serve.sh; chmod +x $sh && $sh 1236 ./html/public.html",
    "postinstall": "yarn add -D glob-parent --ignore-scripts"
  },
